<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="buthod.tony.rentingmanager.MainActivity">

    <Spinner
        android:id="@+id/list_subrents"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </Spinner>

    <FrameLayout
        android:id="@+id/caldroid_container"
        android:layout_below="@id/list_subrents"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </FrameLayout>

    <TextView
        android:id="@+id/label_tenants"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/caldroid_container"
        android:text="@string/tenants"/>

    <TextView
        android:id="@+id/tenants"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/label_tenants"/>

</RelativeLayout>
